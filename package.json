{
  "name": "@guyroyse/ts-optional",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/optional.cjs",
  "module": "./dist/optional.js",
  "types": "./dist/optional.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/optional.js",
      "require": "./dist/optional.cjs"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "lib/optional.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "outdir": "dist"
  },
  "author": "Guy Royse <guy@guyroyse.com> (http://guyroyse.com/)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
